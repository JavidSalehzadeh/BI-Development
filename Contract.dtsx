                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	vw.ContractGuid,
	vw.[ContractBaseGuid] ,
	vw.[ContractNo],
	vw.[ContractSubject],
	vw.[State],
	case when vw.state = '3BAA631B-98A2-4506-9100-5BCFE46E7DDD' then N'پیش نویس'
	     when vw.state = '94807172-F9C6-46F2-BB3B-5AB3CDDB87E5' then N'پیش نویس ابطال شده'
		 when vw.state = 'F93A612F-3B27-4633-BDBA-37473EEDA96D' then N'امضا شده'
    	 when vw.state = 'F0938EDE-2ED7-4F4F-B3A0-9B6086B17948' then N'شروع شده'
		 when vw.state = '11EB69C3-BB07-496F-87F0-99B839B98AF1' then N'معلق'
		 when vw.state = '6F841125-8BEA-4CD7-BBC9-4B968FB87F96' then N'کامل شده'
	     when vw.state = '9198651C-A919-4084-80AA-8C96583597FA' then N'فسخ شده توسط پیمانکار'
	     when vw.state = '5584D8A7-BEB9-4967-8D7B-1B9216750A39' then N'فسخ شده با توافق دو طرفه'
	     when vw.state = '37E80460-B61A-4AB7-9912-34E3BFCAB200' then N'فسخ شده توسط کارفرما'
		 when vw.state = '5361A951-B406-4559-BD1B-DFEC959AD322' then N'در دوره گارانتی'
	     when vw.state = 'A0A6C045-F891-49C9-9A2D-060E71FCEFCE' then N'تعهد شده'
		 when vw.state = 'C9FB4048-CE6E-4314-8F92-EC88E8A7FA56' then N'در انتظار تایید تعهد'
		 when vw.state = '46815E71-6494-4B9E-B77F-5F41A1F9A788' then N'انتقال به سازمان دیگر'
		 else N'نامشخص' end as StateTitle,
	vw.[ContractTypeGuid],
	isnull(vw.ContractTypeTitle,N'نامشخص') ContractTypeTitle,
	case when contractserviceitemselectiontype=1 then N'تعداد و مبلغ مشخص' 
	     when contractserviceitemselectiontype=2 then N'تعداد و مبلغ نامشخص' 
		 else N'نامشخص' end as ContractServiceItemSelectionTypeTitle,
	case when nature=1 then N'هزینه ای'
	     when nature=2 then N'درآمدی'
