                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--DimDetailType
with cte as	(SELECT 
			CASE WHEN FinancialCalculations.Subject = 3 THEN '6A5BE2B3-8BEB-449F-AB11-681673DD5754'
				 WHEN FinancialCalculations.Subject = 4 THEN paymentfactors.guid
				 WHEN FinancialCalculations.Subject = 5 THEN ct.guid
				 WHEN FinancialCalculations.Subject = 6 THEN '2372678D-D20E-4450-904A-39032DD42A30'
				 WHEN FinancialCalculations.Subject = 8 THEN FiscalSubtractionAdditions.guid
				 WHEN FinancialCalculations.Subject = 9 THEN 'D1B18B4D-B464-40DA-809E-952A8F74DD2A'
				 WHEN FinancialCalculations.Subject = 10 THEN '50B192E6-3FE0-49D5-9088-0F1DD076F6BA'
				 WHEN FinancialCalculations.Subject = 11 THEN '17E4299E-5535-4F51-B531-7889E1016763'
				 WHEN FinancialCalculations.Subject = 12 THEN 'BB34601A-9A35-4A7C-9C45-5BC7D2DAE798'
				 WHEN FinancialCalculations.Subject = 13 THEN '22ACEFA9-DF24-44AE-80E5-94278D91EF90'
				 else null end as DetailTypeGuid,
			CASE WHEN FinancialCalculations.Subject = 3 THEN 'علی الحساب'
				 WHEN FinancialCalculations.Subject = 4 THEN paymentfactors.Title
				 WHEN FinancialCalculations.Subject = 5 THEN ct.name
				 WHEN FinancialCalculations.Subject = 6 THEN 'حسن انجام کار'
				 WHEN FinancialCalculations.Subject = 8 THEN FiscalSubtractionAdditions.Title
				 WHEN FinancialCalculations.Subject = 9 THEN 'تخفیف'
				 WHEN FinancialCalculations.Subject = 10 THEN 'تخفیف زمان اجرا'
				 WHEN FinancialCalculations.Subject = 11 THEN 'کسر بابت مفاصاحساب'
				 WHEN FinancialCalculations.Subject = 12 THEN 'تضمین انجام تعهدات'
				 WHEN FinancialCalculations.Subject = 13 THEN 'حق بالاسری'
				 else null end as DetailTypeTitle,
				 FinancialCalculations.subject,FinancialCalculations.type
    FROM    dbo.cnt_FinancialCalculations FinancialCalculations
			INNER JOIN dbo.cnt_vwLatestContractVersions latestContractVersions ON FinancialCalculations.ContractBaseGuid = latestContractVersions.ContractBaseGuid
			LEFT JOIN dbo.cnt_ContractParameters contractParameters ON contractParameters.ContractGuid = latestContractVersions.[ContractParameterContractGuid]
            LEFT JOIN [dbo].[gen_FiscalSubtractionAdditions] FiscalSubtractionAdditions ON FiscalSubtractionAdditions.Guid = FinancialCalculations.FiscalSubtractionAdditionGuid
            LEFT JOIN dbo.cnt_ContractCosts ContractCosts ON ContractCosts.FinancialCalculationGuid = FinancialCalculations.Guid
			LEFT JOIN dbo.cnt_CostTypes ct ON ContractCosts.[CostTypeGuid] = ct.[Guid]
			LEFT JOIN cnt_RequestPartPayments requestPartPayments ON requestPartPayments.FinancialCalculationGuid = FinancialCalculations.Guid
			LEFT JOIN cnt_RequestPayments requestPayments ON requestPayments.FinancialCalculationGuid = FinancialCalculations.Guid
			LEFT JOIN dbo.cnt_ContractPayments contractPayments ON requestPayments.[ContractPaymentGuid] = contractPayments.[Guid]
			LEFT JOIN dbo.cnt_PaymentFactors paymentfactors ON contractPayments.[PaymentFactorGuid] = paymentfactors.[Guid] 
			OUTER APPLY cnt_udfGetFinancialCalculationsAmountInfo(FinancialCalculations.Guid) FinancialCalculationsAmountInfo
	WHERE FinancialCalculations.Deleted = 0 AND contractParameters.Deleted = 0
	)
	select distinct DetailTypeGuid,DetailTypeTitle,Subject from cte</property>
