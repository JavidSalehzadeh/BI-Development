		latestContractVersions.ContractGuid,generalInfos.Amount as ContractAmount,generalInfos.MinusPlusCoefficient,	
    	ci.TotalContractItemsAmount,cv.TotalContractServicesAmount,cp.TotalContractPriceListItemsAmount,
		isnull(ci.TotalContractItemsAmount,0) + isnull(cv.TotalContractServicesAmount,0) + isnull(cp.TotalContractPriceListItemsAmount,0) as FirstAmount,
         --   FiscalSubtractionAdditions.Title AS FiscalSubtractionAdditionTitle ,
         --   ct.Name CostTitle,
			CASE WHEN FinancialCalculations.Subject = 3 THEN '6A5BE2B3-8BEB-449F-AB11-681673DD5754'
				 WHEN FinancialCalculations.Subject = 4 THEN paymentfactors.guid
				 WHEN FinancialCalculations.Subject = 5 THEN ct.guid
				 WHEN FinancialCalculations.Subject = 6 THEN '2372678D-D20E-4450-904A-39032DD42A30'
				 WHEN FinancialCalculations.Subject = 8 THEN FiscalSubtractionAdditions.guid
				 WHEN FinancialCalculations.Subject = 9 THEN 'D1B18B4D-B464-40DA-809E-952A8F74DD2A'
				 WHEN FinancialCalculations.Subject = 10 THEN '50B192E6-3FE0-49D5-9088-0F1DD076F6BA'
				 WHEN FinancialCalculations.Subject = 11 THEN '17E4299E-5535-4F51-B531-7889E1016763'
				 WHEN FinancialCalculations.Subject = 12 THEN 'BB34601A-9A35-4A7C-9C45-5BC7D2DAE798'
				 WHEN FinancialCalculations.Subject = 13 THEN '22ACEFA9-DF24-44AE-80E5-94278D91EF90'
				 else null end as DetailTypeGuid,
			CASE WHEN FinancialCalculations.Subject = 3 THEN 'علی الحساب'
				 WHEN FinancialCalculations.Subject = 4 THEN paymentfactors.Title
				 WHEN FinancialCalculations.Subject = 5 THEN ct.name
				 WHEN FinancialCalculations.Subject = 6 THEN 'حسن انجام کار'
				 WHEN FinancialCalculations.Subject = 8 THEN FiscalSubtractionAdditions.Title
				 WHEN FinancialCalculations.Subject = 9 THEN 'تخفیف'
				 WHEN FinancialCalculations.Subject = 10 THEN 'تخفیف زمان اجرا'
				 WHEN FinancialCalculations.Subject = 11 THEN 'کسر بابت مفاصاحساب'
				 WHEN FinancialCalculations.Subject = 12 THEN 'تضمین انجام تعهدات'
				 WHEN FinancialCalculations.Subject = 13 THEN 'حق بالاسری'
				 else null end as DetailTypeTitle,
			CASE WHEN FinancialCalculations.Subject = 3 THEN requestPartPayments.ApprovedAmount
				 WHEN FinancialCalculations.Subject = 4 THEN requestPayments.ApprovedAmount
				 WHEN FinancialCalculations.Subject = 5 THEN ContractCosts.ApprovedAmount
	             WHEN FinancialCalculations.Subject = 12 THEN
	                            case when generalInfos.Amount = 0 then 	
								                    case when generalInfos.MinusPlusCoefficient=0 then (isnull(ci.TotalContractItemsAmount,0) + isnull(cv.TotalContractServicesAmount,0) + isnull(cp.TotalContractPriceListItemsAmount,0) * contractParameters.SupplyBondPercent)/100
								                         else (generalInfos.MinusPlusCoefficient * isnull(ci.TotalContractItemsAmount,0) + isnull(cv.TotalContractServicesAmount,0) + isnull(cp.TotalContractPriceListItemsAmount,0) * contractParameters.SupplyBondPercent)/100
														 end
                                     else (generalInfos.Amount * contractParameters.SupplyBondPercent)/100 end
				 ELSE FinancialCalculationsAmountInfo.TotalCalculatedAmount
			     END AS TotalAmount,		
			CASE WHEN FinancialCalculations.Subject = 3 THEN (ISNULL(requestPartPayments.ApprovedAmount,0) - FinancialCalculationsAmountInfo.TotalCalculatedAmount) 
				 WHEN FinancialCalculations.Subject = 4 THEN (ISNULL(requestPayments.ApprovedAmount,0) - FinancialCalculationsAmountInfo.TotalCalculatedAmount)
				 WHEN FinancialCalculations.Subject = 5 THEN (ISNULL(ContractCosts.ApprovedAmount,0) - FinancialCalculationsAmountInfo.TotalCalculatedAmount)
				 WHEN FinancialCalculations.Subject = 12 THEN
	                            case when generalInfos.Amount = 0 then 	
								                    case when generalInfos.MinusPlusCoefficient=0 then (isnull(ci.TotalContractItemsAmount,0) + isnull(cv.TotalContractServicesAmount,0) + isnull(cp.TotalContractPriceListItemsAmount,0) * contractParameters.SupplyBondPercent)/100 - FinancialCalculationsAmountInfo.TotalCalculatedAmount
								                         else (generalInfos.MinusPlusCoefficient * isnull(ci.TotalContractItemsAmount,0) + isnull(cv.TotalContractServicesAmount,0) + isnull(cp.TotalContractPriceListItemsAmount,0) * contractParameters.SupplyBondPercent)/100 - FinancialCalculationsAmountInfo.TotalCalculatedAmount
														 end
                                     else (generalInfos.Amount * contractParameters.SupplyBondPercent)/100 - FinancialCalculationsAmountInfo.TotalCalculatedAmount end
				 ELSE FinancialCalculationsAmountInfo.RemainAmount
			END AS RemainAmount,
			FinancialCalculationsAmountInfo.TotalAdditionAmount,
			FinancialCalculationsAmountInfo.TotalSubtractionAmount
			--,requestPayments.RequestDate AS RequestPaymentsDate,
			--paymentfactors.Title PaymentFactorTitle,
			--requestPayments.Description RequestPaymentsDescription,
			--requestPayments.Amount RequestPaymentsApprovedAmount,
			--requestPayments.Amount RequestPaymentsAmount,
			--requestPartPayments.ApprovedAmount RequestPartPaymentsApprovedAmount,
			--requestPartPayments.PaidAmount RequestPartPaymentsPaidAmount,
			--requestPartPayments.RequestAmount RequestPartPaymentsRequestAmount,
			--requestPartPayments.RequestDate RequestPartPaymentsRequestDate,
			--requestPartPayments.ConfirmedDate RequestPartPaymentsConfirmedDate,
			--requestPartPayments.PaymentDate RequestPartPaymentsPaymentDate,
			--requestPartPayments.Description RequestPartPaymentsDescription
    FROM    dbo.cnt_FinancialCalculations FinancialCalculations
			INNER JOIN dbo.cnt_vwLatestContractVersions latestContractVersions ON FinancialCalculations.ContractBaseGuid = latestContractVersions.ContractBaseGuid
			INNER JOIN dbo.cnt_ContractGeneralInfos generalInfos ON latestContractVersions.ContractGeneralInfoContractGuid = generalInfos.ContractGuid
			LEFT JOIN dbo.cnt_ContractParameters contractParameters ON contractParameters.ContractGuid = latestContractVersions.[ContractParameterContractGuid]
      LEFT JOIN [dbo].[gen_FiscalSubtractionAdditions] FiscalSubtractionAdditions ON FiscalSubtractionAdditions.Guid = FinancialCalculations.FiscalSubtractionAdditionGuid
      LEFT JOIN dbo.cnt_ContractCosts ContractCosts ON ContractCosts.FinancialCalculationGuid = FinancialCalculations.Guid
			LEFT JOIN dbo.cnt_CostTypes ct ON ContractCosts.[CostTypeGuid] = ct.[Guid]
			LEFT JOIN cnt_RequestPartPayments requestPartPayments ON requestPartPayments.FinancialCalculationGuid = FinancialCalculations.Guid
			LEFT JOIN cnt_RequestPayments requestPayments ON requestPayments.FinancialCalculationGuid = FinancialCalculations.Guid
			LEFT JOIN dbo.cnt_ContractPayments contractPayments ON requestPayments.[ContractPaymentGuid] = contractPayments.[Guid]
			LEFT JOIN dbo.cnt_PaymentFactors paymentfactors ON contractPayments.[PaymentFactorGuid] = paymentfactors.[Guid] 
			 LEFT JOIN ( SELECT   ContractGuid,SUM([Quantity] * [UnitPrice]) as TotalContractItemsAmount
                          FROM    cnt_ContractItems
	                      WHERE  ISNULL([Deleted], 0) = 0 AND [IsConditional] = 0
		                  group by contractguid) as  ci
		                  on ci.ContractGuid = latestContractVersions.ContractGuid

			 LEFT JOIN ( SELECT   ContractGuid,SUM([Quantity] * [UnitPrice]) as TotalContractServicesAmount
                          FROM    cnt_ContractServices
	                      WHERE  ISNULL([Deleted], 0) = 0 AND [IsConditional] = 0
		                  group by contractguid) as  cv
		                  on cv.ContractGuid = latestContractVersions.ContractGuid

			 LEFT JOIN ( SELECT   ContractGuid,SUM([Quantity]*[UnitPrice]*[Coefficient]) as TotalContractPriceListItemsAmount
                          FROM    cnt_ContractPriceListItems cp
	                      WHERE  ISNULL([Deleted], 0) = 0 AND [IsConditional] = 0
		                  group by contractguid) as  cp
		                  on cp.ContractGuid = latestContractVersions.ContractGuid

			OUTER APPLY cnt_udfGetFinancialCalculationsAmountInfo(FinancialCalculations.Guid) FinancialCalculationsAmountInfo
	WHERE FinancialCalculations.Deleted = 0 AND contractParameters.Deleted = 0
)
select ContractBaseGuid,DetailTypeGuid,DetailTypeTitle,sum(isnull(TotalAmount,0)) TotalAmount,sum(isnull(RemainAmount,0)) RemainAmount,
sum(isnull(TotalAdditionAmount,0)) TotalAdditionAmount,sum(isnull(TotalSubtractionAmount,0)) TotalSubtractionAmount
from cte where TotalAmount is not null
group by ContractBaseGuid,DetailTypeGuid,DetailTypeTitle</property>
