                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  Distinct contractItems.Guid,vw.ContractBaseGuid,1 as Type
	,contractItems.Quantity,
	contractItems.Unitprice,contractItems.ItemGuid,'00000000-0000-0000-0000-000000000000' AS ServiceGuid,contractItems.DiscountQuantity,contractItems.DiscountAmount,
	contractItems.DiscountPercent,isnull(contractItems.DiscountRemained,0) as DiscountRemained ,isnull(contractItems.ContractProjectGuid,'00000000-0000-0000-0000-000000000000') as ProjectGuid
	,isnull(contractItems.ContractCostCenterGuid,'00000000-0000-0000-0000-000000000000') as CostCenterGuid
	,isnull(contractItems.ContractPlanGuid,'00000000-0000-0000-0000-000000000000') as PlanGuid,
            CAST(contractItems.[Quantity] * contractItems.[UnitPrice] AS DECIMAL(30,
                                                              3)) AS [TotalPrice] ,
            --items.[Title] ,
            --items.[Code] ,
            --items.[MeasurementUnitTitle] ,
            --items.[Code] + ': ' + items.[Title] AS [ItemDisplayTitle] ,

            CASE WHEN vw.ContractServiceItemSelectionType = 2
                 THEN 0
                 ELSE ISNULL(contractItems.Quantity
                             - contractItems.DiscountQuantity
                             - ISNULL(TotalNumberOfWorkStatementItem.TotalItemQuantity,0),
                             0)
            END AS RemainedNumbers,
		isnull(TotalNumberOfWorkStatementItem.TotalItemQuantity,0.000) AS TotalQuantity
    FROM    cnt_ContractItems contractItems
         --   LEFT JOIN lcm_vwItems items ON contractItems.[ItemGuid] = items.[Guid]
        join cnt_vwcontracts vw on  contractItems.contractguid = vw.ContractGuid
		LEFT JOIN( 
			SELECT  WorkStatementItemGuid,ISNULL(SUM(FinalQuantity),0) AS TotalItemQuantity
			FROM    dbo.cnt_WorkStatementItems
			WHERE   Deleted = 0
            GROUP BY WorkStatementItemGuid
		     )TotalNumberOfWorkStatementItem ON  TotalNumberOfWorkStatementItem.WorkStatementItemGuid = contractItems.Guid
			where contractItems.deleted = 0 

union

    SELECT DISTINCT
           contractServices.Guid,vw.ContractBaseGuid,2 as Type
	,contractServices.Quantity,
	contractServices.Unitprice,'00000000-0000-0000-0000-000000000000' AS ItemGuid,contractServices.Serviceguid,contractServices.DiscountQuantity,contractServices.DiscountAmount,
	contractServices.DiscountPercent,isnull(contractServices.DiscountRemained,0) as DiscountRemained,
	
isnull(contractServices.ContractProjectGuid,'00000000-0000-0000-0000-000000000000') as ProjectGuid
	,isnull(contractServices.ContractCostCenterGuid,'00000000-0000-0000-0000-000000000000') as CostCenterGuid
	,isnull(contractServices.ContractPlanGuid,'00000000-0000-0000-0000-000000000000') as PlanGuid,
            CAST(contractServices.[Quantity] * contractServices.[UnitPrice] AS DECIMAL(30,
                                                              3)) AS [TotalPrice] ,
            --[services].[Title] AS [ServiceTitle] ,
            --[services].[Code] AS [ServiceCode] ,
            --CAST([services].[Code] AS NVARCHAR) + ': ' + [services].[Title] AS [ServiceDisplayTitle] ,

            CASE WHEN vw.ContractServiceItemSelectionType = 2
                 THEN 0
                 ELSE ISNULL(contractServices.Quantity
                             - contractServices.DiscountQuantity
                             - ISNULL(TotalNumberOfWorkStatementService.TotalServiceQuantity,0),
                             0)
            END AS RemainedNumbers,
			isnull(TotalNumberOfWorkStatementService.TotalServiceQuantity,0.000) AS TotalQuantity
			--project.Title ProjectTitle,
			--[plan].Title PlanTitle,
			--costCenter.Title CostCenterTitle
    FROM    cnt_ContractServices contractServices
           join cnt_vwcontracts vw on contractServices.ContractGuid = vw.contractguid
      --      LEFT JOIN lcm_Services [services] ON contractServices.[ServiceGuid] = [services].[Guid]
            LEFT JOIN (
					SELECT  WorkStatementServiceGuid,ISNULL(SUM(FinalQuantity),0) AS TotalServiceQuantity
					FROM    dbo.cnt_WorkStatementItems
					WHERE   Deleted = 0
					GROUP BY WorkStatementServiceGuid
           
			) TotalNumberOfWorkStatementService  ON TotalNumberOfWorkStatementService.WorkStatementServiceGuid = contractServices.Guid</property>
